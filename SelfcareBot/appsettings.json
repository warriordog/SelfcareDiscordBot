{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "DSharpPlus": "Information",
            "SelfcareBot": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        },
        "Console": {
            "FormatterName": "simple",
            "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "[yyyy-MM-dd HH:mm:ss.fff K] "
            }
        }
    },
    "HydrationOptions": {
        "LeaderboardSize": 3,
        "WaterEmojiName": ":droplet:",
        "HydrateRequestExpiresAfter": "0:0:5",
        "DeleteExpiredHydrationRequests": true
    },
    "BotOptions": {
        "REM": "Set token in user secrets via 'dotnet user-secrets set \"BotOptions:DiscordToken\" \"INSERT TOKEN HERE\"'.",
        "DiscordToken": null,
        "CommandPrefixes": [ "care*" ]
    },
    "SelfcareDatabaseOptions": {
        "ConnectionString": "DataSource=selfcare.db"
    }
}